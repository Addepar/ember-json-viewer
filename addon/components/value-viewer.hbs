{{#if this.isPrimitive}}
  <span class="value primitive">
    <SimpleValue @value={{@value}} />
  </span>
{{else}}
  <span class="prefix">
    {{this.prefix}}
  </span>

  {{#if @showSummary}}
    <span class="entries-summary">{{this.valueSummary}}</span>
  {{else}}
    <ul class="entries depth-{{@depth}} {{if @showInline "entries-inline"}}">
      {{#if this.isObj}}
        {{#each-in @value as |key value|}}
          <EntryViewer @key={{key}} @value={{value}} @depth={{@depth}} />
        {{/each-in}}
      {{else}}
        {{#each @value as |value|}}
          <li class="entry">
            <ValueViewer @value={{value}} @depth={{@depth}} />
            <span class="entry-delimiter">,</span>
          </li>
        {{/each}}
      {{/if}}
    </ul>
  {{/if}}

  <span class="suffix">
    {{this.suffix}}
  </span>
{{/if}}