{
  "name": "ember-json-viewer",
  "version": "0.7.0",
  "description": "A JSON-viewer addon for Ember apps",
  "keywords": [
    "ember-addon"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "repository": "https://github.com/Addepar/ember-json-viewer",
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
