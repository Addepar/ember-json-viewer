    "eslint-plugin-qunit": "^8.1.2",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.4.2",
    "qunit": "^2.23.1",
    "qunit-dom": "^3.4.0",
    "release-it": "^14.2.2",
    "webpack": "^5.97.1"
  },
  "engines": {
    "node": ">= 18"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "http://addepar.github.io/ember-json-viewer"
  },
  "volta": {
    "node": "18.20.5",
    "yarn": "1.22.22"
  }
