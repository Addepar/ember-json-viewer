    "ember-resolver": "^11.0.1",
    "ember-source": "~4.12.4",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^2.21.0",
    "ember-try": "^3.0.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-ember": "^12.3.3",
